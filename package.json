{
	"name": "dashboardyeah",
	"displayName": "Dashboard",
	"description": "Add a project dashboard to VSCode for 1 click launching of workspaces. Supports remote development projects.",
	"publisher": "bobmagicii",
	"author": {
		"name": "Bob Majdak Jr",
		"email": "bob@pegasusgate.net"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bobmagicii/vscode-dashboard"
	},
	"version": "1.0.0-beta3",
	"icon": "local/gfx/icon-lg.png",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"galleryBanner": {
		"color": "#262335",
		"theme": "dark"
	},
	"keywords": [
		"project",
		"dashboard",
		"switchboard",
		"project management"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/main.js",
	"contributes": {
		"commands": [],
		"configuration": {
			"title": "Dashboard",
			"properties": {
				"dashboardyeah.debug": {
					"type": "boolean",
					"default": false,
					"description": "Enable debugging output."
				},
				"dashboardyeah.showPaths": {
					"type": "boolean",
					"default": true,
					"description": "Show paths on the project entries."
				},
				"dashboardyeah.openOnNewWindow": {
					"type": "boolean",
					"default": true,
					"description": "Auto open the dashboard when spawning new windows."
				},
				"dashboardyeah.title": {
					"type": "string",
					"default": "Projects",
					"description": "Dashboard Title"
				},
				"dashboardyeah.fontSize": {
					"type": "string",
					"default": "font-size-normal",
					"description": "Text Size"
				},
				"dashboardyeah.folderSizing": {
					"type": "string",
					"default": "col-12",
					"description": "The Bootstrap CSS classes used to adjust how the folders scale to your window size."
				},
				"dashboardyeah.columnSizing": {
					"type": "string",
					"default": "col-12 col-md-6",
					"description": "The Bootstrap CSS classes used to adjust how the columns scale to your window size."
				},
				"dashboardyeah.database": {
					"type": "array",
					"default": [],
					"description": "List of projects stored in your user config so that it can be synced."
				}
			}
		},
		"views": {
			"dashboardyeah-dashboard-sidebar": [
				{
					"id": "dashboardyeah-dashboard-sidebar",
					"type": "webview",
					"name": "Projects",
					"icon": "local/gfx/icon.svg",
					"visibility": "visible"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dashboardyeah-dashboard-sidebar",
					"title": "Projects",
					"icon": "local/gfx/icon.svg"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/color": "^3.0.2",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/uuid": "^8.3.4",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@vscode/codicons": "^0.0.26",
		"color": "^4.2.0",
		"uuid": "^8.3.2"
	}
}
